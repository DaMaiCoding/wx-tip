{
  "name": "wx-tip",
  "productName": "wxTip",
  "version": "1.0.0",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron src/main/index.js",
    "dev": "nodemon --exec electron src/main/index.js",
    "license:check": "license-checker --production --summary --failOn \"GPL;LGPL;AGPL\""
  },
  "build": {
    "appId": "wxTip",
