    },
    "win": {
      "icon": "assets/icon.png",
      "target": [
        "nsis"
      ]
    }
  },
  "keywords": [],
  "author": "wx-tip Contributors",
  "license": "MIT",
  "devDependencies": {
    "electron": "^40.1.0",
    "electron-builder": "^25.1.8",
    "license-checker": "^25.0.1",
    "nodemon": "^3.1.11"
  },
  "dependencies": {
    "body-parser": "^2.2.2",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "express": "^5.2.1"
  }
}
