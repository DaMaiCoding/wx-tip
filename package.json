    "electron": "^40.1.0",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1",
    "license-checker": "^25.0.1",
    "nodemon": "^3.1.11",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "body-parser": "^2.2.2",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "express": "^5.2.1"
  }
