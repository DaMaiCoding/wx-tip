{
  "name": "wx-tip",
  "productName": "wxTip",
  "version": "1.0.1",
  "main": "src/main/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaMaiCoding/wx-tip.git"
  },
  "scripts": {
    "start": "electron src/main/index.js",
    "dev": "electron src/main/index.js",
    "build": "node scripts/build.js",
    "postbuild": "node scripts/clean-dist.js",
    "build:win": "node scripts/smart-build.js",
    "release": "node scripts/release.js",
    "build:mac": "npm run build && electron-builder --mac --config electron-builder.config.js --publish never",
    "build:portable": "electron-builder --win --publish never",
    "clean": "rimraf dist",
    "license:check": "license-checker --production --summary --failOn \"GPL;LGPL;AGPL\""
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron"
    ]
  },
  "keywords": [],
  "author": "wx-tip Contributors",
  "license": "MIT",
  "devDependencies": {
    "7zip-bin": "^5.2.0",
    "cross-env": "^10.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1",
    "license-checker": "^25.0.1",
    "nodemon": "^3.1.11",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "body-parser": "^2.2.2",
    "electron-log": "^5.4.3",
    "express": "^5.2.1"
  }
}